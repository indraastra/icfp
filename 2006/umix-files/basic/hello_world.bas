V        DIM i AS INTEGER
X        i = I
XV       DIM j AS INTEGER
XX       j = XLIX
XXV      DIM k AS INTEGER
XXX      k = XLVIII
XXXV     PRINT "checking words(" + i + ") + '" + CHR(j) + CHR(k) + "'" + CHR(X)
XL       k = k + I
XLV      IF k > LVII THEN GOTO LV
L        GOTO XXXV
LV       j = j + I
LX       IF j > LVII THEN GOTO LXX
LXV      GOTO XXX
LXX      i = i + I
LXXV     IF i > III THEN GOTO LXXXV
LXXX     GOTO XX
LXXXV    PRINT "DONE!" + CHR(X)
